<?php
 //no 1
$aaaa = array(
 
'paket data 1' => array('Nama data' => array('smartfreen'), 'kuota' => array('unlimited')),
 
'paket data 2' => array('Nama data' => array('telkomsel'), 'kuota' => array('10 GB')),
 
'paket data 3' =>array('Nama data' => array('xl combo'), 'kuota' => array('6 GB'))
 
);

//menampilkan perulangan array multidimensi
foreach($aaaa as $name => $data){
 echo $name . '<br/>';
 foreach($data as $name2 => $value){
 echo $name2 . ' : ' . $value[0] .'<br> ';
 }
 echo '<br/>	';
}
ECHO '-------------------------------------------------------------------------';
 echo '<br/>';
 
